<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jd.demo.mapper.DeptMapper">

    <select id="getAllDept" resultType="Dept">
        SELECT id, name FROM dept
    </select>

    <update id="updateOne">
        update dept set name = "test1" where id = 1
    </update>

    <update id="updateTwo">
        update dept set name = "test2" where id = 2
    </update>
</mapper>